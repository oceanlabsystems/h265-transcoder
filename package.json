{
  "name": "h265-transcoder",
  "version": "1.7.7",
  "description": "An application to batch process videofiles into time based chunks and output to H.265",
  "main": "./out/main/index.js",
  "bin": {
    "h265-transcoder-cli": "./out/cli/cli/index.js"
  },
  "author": {
    "name": "Oceanlab Systems",
    "email": "support@oceanlabsystems.com"
  },
  "homepage": "https://www.oceanlabsystems.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/oceanlabsystems/h265-transcoder.git"
  },
  "bugs": {
    "url": "https://github.com/oceanlabsystems/h265-transcoder/issues"
  },
  "keywords": [
    "h265",
    "hevc",
    "video",
    "transcoder",
    "encoder",
    "gstreamer",
    "electron",
    "batch-processing",
    "cli",
    "service"
  ],
  "license": "MIT",
  "build": {
    "appId": "com.oceanlabsystems.h265-transcoder",
    "productName": "H265 Transcoder",
    "directories": {
      "output": "dist"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "include": "installer/nsis-installer.nsh",
      "customNsisBinary": null
    },
    "win": {
      "icon": "resources/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "mac": {
      "icon": "resources/icon.png",
      "category": "public.app-category.utilities",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "linux": {
      "icon": "resources/icon.png",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ],
      "category": "Utility"
    },
    "files": [
      "out/**/*",
      "resources/**/*",
      "gstreamer/**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!gstreamer/**/*.msi"
    ],
    "extraResources": [
      {
        "from": "gstreamer",
        "to": "gstreamer",
        "filter": [
          "**/*",
          "!**/*.msi",
          "!**/*.pkg"
        ]
      },
      {
        "from": "installer/service",
        "to": "service",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck:cli": "tsc --noEmit -p tsconfig.cli.json",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "check:gstreamer:linux": "node scripts/check-gstreamer-linux.js",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "download-gstreamer": "node scripts/download-gstreamer.js",
    "prebuild": "npm run download-gstreamer",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && npm run cli:build && electron-builder --win --x64 --ia32",
    "build:win32": "npm run build && npm run cli:build && electron-builder --win --ia32",
    "build:win64": "npm run build && npm run cli:build && electron-builder --win --x64",
    "prebuild:mac": "node scripts/download-gstreamer.js --platform darwin",
    "build:mac": "npm run prebuild:mac && electron-vite build && npm run cli:build && electron-builder --mac",
    "prebuild:linux": "node -e \"process.exit(process.env.BUNDLE_GSTREAMER_LINUX==='1'?0:1)\" && bash scripts/build-gstreamer-linux-bundle.sh || echo 'Linux: using system GStreamer (set BUNDLE_GSTREAMER_LINUX=1 to bundle)'",
    "build:linux": "npm run prebuild:linux && electron-vite build && npm run cli:build && electron-builder --linux",
    "generate-icons": "node scripts/generate-icons.js",
    "release-major": "npx standard-version --release-as major && git push --follow-tags origin main",
    "release-minor": "npx standard-version --release-as minor && git push --follow-tags origin main",
    "release-patch": "npx standard-version --release-as patch && git push --follow-tags origin main",
    "commit": "npx cz",
    "cli:dev": "ts-node src/cli/index.ts",
    "cli:build": "tsc -p tsconfig.cli.json",
    "cli:start": "node out/cli/cli/index.js",
    "cli:package-linux": "bash scripts/build-linux-cli.sh",
    "docker:build": "npm run cli:build && docker build -t h265-transcoder .",
    "docker:run": "docker run -v ./input:/input -v ./output:/output -v ./config:/config h265-transcoder"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "child_process": "^1.0.2",
    "chokidar": "^3.6.0",
    "clsx": "^2.1.1",
    "commander": "^12.1.0",
    "sonner": "^2.0.3",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.5.1",
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@heroicons/react": "^2.1.5",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "antd": "^5.21.4",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "framer-motion": "^11.11.9",
    "lucide-react": "^0.453.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.27.0",
    "sharp": "^0.34.5",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.13",
    "to-ico": "^1.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vite": "^5.3.1",
    "yarn": "^1.22.22"
  }
}
